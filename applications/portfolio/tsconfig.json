        "target": "es6",
        "noImplicitAny": false,
        "sourceMap": false,
        "listFiles":true,
        "baseUrl": ".",
        "paths": {
            "@components/*": ["./config/components/*"],
            "@interfaces/*": ["./config/interfaces/*"]
